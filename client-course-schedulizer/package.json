{
  "name": "client-course-schedulizer",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build": "snowpack build --polyfill-node",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "start": "snowpack dev --polyfill-node",
    "test": "web-test-runner \"src/**/*.test.tsx\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fullcalendar/common": "5.5.1",
    "@fullcalendar/core": "5.5.1",
    "@fullcalendar/interaction": "5.5.0",
    "@fullcalendar/moment": "5.5.0",
    "@fullcalendar/react": "5.5.0",
    "@fullcalendar/timegrid": "5.5.1",
    "@harmoniously/react": "0.0.2-alpha.0",
    "@hookform/resolvers": "1.3.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "fs": "0.0.1-security",
    "harmoniously": "0.0.2-alpha.0",
    "js-file-download": "^0.4.12",
    "lodash": "4.17.20",
    "material-ui-image": "3.3.1",
    "material-ui-popup-state": "^1.7.0",
    "moment": "2.29.1",
    "object-hash": "^2.1.1",
    "papaparse": "^5.3.0",
    "path": "^0.12.7",
    "randomcolor": "^0.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.9.6",
    "react-router-dom": "5.2.0",
    "react-spring": "8.0.27",
    "react-stick": "4.1.1",
    "react-stickynode": "3.0.4",
    "react-table": "^7.6.1",
    "react-visibility-sensor": "5.1.1",
    "runtypes": "5.0.1",
    "xlsx": "0.16.9",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "@snowpack/plugin-sass": "1.3.0",
    "@snowpack/plugin-typescript": "^1.2.0",
    "@snowpack/plugin-webpack": "2.3.0",
    "@snowpack/web-test-runner-plugin": "^0.2.0",
    "@testing-library/react": "^11.0.0",
    "@types/chai": "^4.2.13",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/node": "12.12.62",
    "@types/object-hash": "1.3.4",
    "@types/papaparse": "^5.2.4",
    "@types/randomcolor": "0.5.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/react-stickynode": "3.0.1",
    "@types/react-table": "^7.0.25",
    "@types/snowpack-env": "^2.3.2",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "@web/test-runner": "^0.12.0",
    "chai": "^4.2.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
